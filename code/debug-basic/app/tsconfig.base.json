{
    "compilerOptions": {
      "target": "es2022",
      "module": "commonjs",
      "lib": [
        "es6"
      ],
      "allowJs": true,
      "strict": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true,
      "types": [
        "node", "jest"
      ],
      "skipLibCheck": true,
      "sourceMap": true
    }
  }